package co.edu.javerianacali.entities;

/**
 *
 * @author afgomez
 */
public class AtributoValor {

    public String atributo;  //nombre del atributo en java-metodologia.
    public String valor;     // valor del atributo.

    /**
     * Creates a new instance of Atributovalor
     */
    public AtributoValor(String pAtributo, String pValor) {
        atributo = pAtributo;
        valor = pValor;
    }

    public String getAtributo() {
        return this.atributo;
    }

    public String getValor() {
        return this.valor;
    }

    public void setAtributo(String pAtributo) {
        atributo = pAtributo;
    }

    public void setValor(String pValor) {
        valor = pValor;
    }
}

<!DOCTYPE html>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@taglib uri="http://www.springframework.org/tags/form" prefix="f"%>
<html>
<body>
       <table width="600" border="1" align="left">
             <TR>
                    <c:forEach var="parametro" items="${parametros}">
                           <TD width="20%" class="textoTitulo"><div align="center"><c:out value="${parametro.nombre}"></c:out> </div></TD>
                    </c:forEach>
             </TR>
             <TR>
                    <c:forEach var="parametro" items="${parametros}">
                           <TD width="20%" class="textoTitulo"><div align="center"><c:out value="${parametro.value}"></c:out> </div></TD>
                    </c:forEach>
             </TR>
       </table>
</body>
</html>

